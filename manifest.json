{
    "name": "songfinder",
    "version": "0.1",
    "permissions": ["tabs","activeTab"],

    "content_scripts":[
      {
        "matches":["https://en.wikipedia.org/*", "*://en.wikipedia.org/*"],
        "exclude_globs":["*://youtube.com/*", "*youtube.com/*"],
        "js":["parser.js"],
        "run_at": "document_end"
        }
      ],
    "manifest_version": 3
  }
  